{
  "extends": "google",
  "installedESLint": true,
  "no-undef": 1,
  "no-unused-vars": 1,
  "env": {
    "amd" : true,
    "jasmine": true,
    "browser": true
  },
  "rules": {
    "max-len": [2, 120, 2],
    "linebreak-style": "off",
    "padded-blocks": "off"
  },
  "globals": {
    "inject": true,
    "module": true,
    "requirejs": true
  }
}
